import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int value_1=sc.nextInt();
		int value_2=sc.nextInt();
		int temp;
		System.out.println("The values before swapping:"+value_1+" "+value_2);
		temp=value_1;
		value_1=value_2;
		value_2=temp;
		System.out.println("The values after swapping:"+value_1+" "+value_2);
	}
}

OUTPUT:
The values before swapping:5 7
The values after swapping:7 5
