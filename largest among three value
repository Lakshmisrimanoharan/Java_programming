import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the three value :");
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		int num3=sc.nextInt();
		if(num1 > num2)
		{
		    if(num1 > num3)
		    {
		        System.out.println(num1 +" is the highest value");
		    }
		    else
		    {
		        System.out.println(num3 +" is the highest value");
		    }
		}
		else if(num2 > num3)
		{
		    System.out.println(num2 +" is the highest value");
		}
		else
		{
		    System.out.println(num3 +" is the highest value");
		}
		
		
	}
}

Output :
Enter the three value :10 20 30
30 is the highest value
