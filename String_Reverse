import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String str=sc.nextLine();
		String reverse="";
		int len=str.length();
		for(int i=len- 1;i>=0;i--)
		{
		    reverse=reverse+str.charAt(i);
		}
		
		System.out.println(reverse);
		
	}
}

OUTPUT :
kite is flying
gniylf si etik
